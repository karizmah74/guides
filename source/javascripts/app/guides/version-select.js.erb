var currentRevision = '<%= data.search.revision%>';
$.ajax('/versions.json')
  .done(function(data) {
    var versionSelect = $(".version-select");
    versionSelect.select2({
        data: data
    })
    versionSelect.on('change', function() {
      var version = this.selectedOptions[0].value;

      window.location = '/' + version + '/';
    });
  });
